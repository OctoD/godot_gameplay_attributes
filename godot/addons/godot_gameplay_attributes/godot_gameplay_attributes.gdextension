[configuration]
entry_symbol = "gdext_rust_init"
compatibility_minimum = 4.2
reloadable = true

[libraries]
linux.debug.x86_64 =     "./build/debug/libgodot_gameplay_attributes.so"
linux.release.x86_64 =   "./build/release/libgodot_gameplay_attributes.so"
windows.debug.x86_64 =   "./build/debug/godot_gameplay_attributes.dll"
windows.release.x86_64 = "./build/release/godot_gameplay_attributes.dll"
macos.debug =            "./build/debug/libgodot_gameplay_attributes.dylib"
macos.release =          "./build/release/libgodot_gameplay_attributes.dylib"
macos.debug.arm64 =      "./build/debug/libgodot_gameplay_attributes.dylib"
macos.release.arm64 =    "./build/release/libgodot_gameplay_attributes.dylib"